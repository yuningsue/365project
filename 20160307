/*Project Euler - Project 1 */

    public static void main(String[] argv) {
        int sum=0;
        for(int i=1;i<10;i++)
        {
            if(i%3==0)
            {
                sum += i;
            }
            else if(i%5==0)
            {
                sum += i;
            }
        }
        System.out.println(sum);
    }
