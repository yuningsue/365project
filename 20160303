x = 12.3
y = 4.56
func = 0.
### START YOUR CODE HERE ###
func = ((x**(y)+y**(x))**0.5)/((x**(y))**0.5+(y**(x))**0.5)

#### END YOUR CODE HERE ####
print 'value:',func

